<div id="teams_view_template">
{{#Teams}}
       <div class="panel panel-default">
          <div class="panel-heading">
            <h3 class="panel-title"></h3>
            <div class="media">
              <a class="pull-left" href="#">
                <img class="media-object" src="/img/leader1.jpg" alt="..." height="50px" width="50px">
              </a>
                  <div class="media-body">
                    <div class="col-md-6">
                        <h4 class="media-heading">{{Title}}</h4>
                    {{Description}}
                    </div>
                    <div class="col-md-6 text-right">
                      {{#Tags}}
                        <img src="/img/tags/{{imageName}}" width="30px" height="30px">
                      {{/Tags}}
                    </div>
                    
                </div>
            </div>
          </div>
          <div class="panel-body">
            Looking for: {{lookingFor}}
          </div>
      </div>
{{/Teams}}
</div>